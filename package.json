{
  "name": "angular-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run serve:fractal-worker\" \"npm run serve:sorting-worker\" \"npm run serve:app\"",
    "build": "npm run build:fractal-worker && npm run build:sorting-worker && npm run build:app",
    "serve:fractal-worker": "ng run app:serve-fractal-worker",
    "build:fractal-worker": "ng run app:build-fractal-worker",
    "serve:sorting-worker": "ng run app:serve-sorting-worker",
    "build:sorting-worker": "ng run app:build-sorting-worker",
    "serve:app": "ng serve",
    "build:app": "ng build --prod",
    "report": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.0.0",
    "@angular/cdk": "8.0.1",
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "8.0.0",
    "@angular/material": "8.0.1",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@angular/pwa": "0.800.2",
    "@angular/router": "8.0.0",
    "@angular/service-worker": "8.0.0",
    "@covalent/core": "2.0.2",
    "@swimlane/ngx-charts": "12.0.1",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.7",
    "d3": "5.9.2",
    "hammerjs": "2.0.8",
    "lodash": "4.17.11",
    "rainbowvis.js": "1.0.1",
    "rxjs": "6.4.0",
    "tslib": "1.9.3",
    "web-animations-js": "2.3.1",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.2",
    "@angular/cli": "8.0.2",
    "@angular/compiler-cli": "8.0.0",
    "@angular/language-service": "8.0.0",
    "@types/jasmine": "3.3.12",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash": "4.14.134",
    "@types/node": "8.9.4",
    "codelyzer": "5.1.0",
    "concurrently": "4.1.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "protractor": "5.4.2",
    "ts-loader": "6.0.1",
    "ts-node": "8.2.0",
    "tslint": "5.16.0",
    "typescript": "3.4.5",
    "webpack-bundle-analyzer": "3.3.2"
  }
}
