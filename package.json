{
  "name": "angular-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run serve:fractal-worker\" \"npm run serve:sorting-worker\" \"npm run serve:app\"",
    "build": "npm run build:fractal-worker && npm run build:sorting-worker && npm run build:app",
    "serve:fractal-worker": "ng run app:serve-fractal-worker",
    "build:fractal-worker": "ng run app:build-fractal-worker",
    "serve:sorting-worker": "ng run app:serve-sorting-worker",
    "build:sorting-worker": "ng run app:build-sorting-worker",
    "serve:app": "ng serve",
    "build:app": "ng build --prod",
    "report": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.3",
    "@angular/cdk": "7.0.3",
    "@angular/common": "7.0.3",
    "@angular/compiler": "7.0.3",
    "@angular/core": "11.0.5",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "7.0.3",
    "@angular/http": "7.0.3",
    "@angular/material": "7.0.3",
    "@angular/platform-browser": "7.0.3",
    "@angular/platform-browser-dynamic": "7.0.3",
    "@angular/pwa": "0.10.5",
    "@angular/router": "7.0.3",
    "@angular/service-worker": "7.0.3",
    "@covalent/core": "2.0.0-beta.4",
    "@swimlane/ngx-charts": "10.0.0",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.7",
    "d3": "5.7.0",
    "hammerjs": "2.0.8",
    "lodash": "4.17.11",
    "rainbowvis.js": "1.0.1",
    "rxjs": "6.3.3",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.10.5",
    "@angular/cli": "7.0.5",
    "@angular/compiler-cli": "7.0.3",
    "@angular/language-service": "7.0.3",
    "@types/jasmine": "2.8.11",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash": "4.14.118",
    "@types/node": "8.9.4",
    "codelyzer": "4.5.0",
    "concurrently": "4.1.0",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.4.1",
    "ts-loader": "5.3.1",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.6",
    "webpack-bundle-analyzer": "3.0.3"
  }
}
